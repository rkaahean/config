# pull from master
alias gpl='git pull origin master'

# pip alias
alias pip='pip3'

export TERM=xterm-256color
# GITHUB COMMANDS
# add files to commit
alias ga='git add .'
# commit files
alias gc='git commit -m'
# add and commit generic message
alias gac='git add . && git commit -m "commit"'
# push to master
alias gp='git push origin'
# all in one
alias gacp='git add . && git commit -m "commit" && git push origin'
# git checkout new branch
alias gcb='git checkout -b'

# NPM commands
alias ni='npm install'
alias nci='npm ci'
alias ndev='npm run dev'
alias nstart='npm run start'
alias nbuild='npm run build'
